actor A(5){
	knownactors {
		B b;
	}

	actorvars {
		int i;
		int arr[3];
	}

	msghandler initial() {
		i = 2 + 2;
		b.foo(i);
		i = i + 4;
	}

	msghandler bar(){
	    int ar[5];
		sender.foo(i+1);
		print(self.arr[1] + 2 * 3 + i);
	}
}

actor B(2){
	knownactors {
		A a;
	}

	actorvars {
	    int x;
	}

	msghandler foo(int i) {
		sender.bar(i+1);
	}
}

main {
	A a(b):();
	B b(a):();
}